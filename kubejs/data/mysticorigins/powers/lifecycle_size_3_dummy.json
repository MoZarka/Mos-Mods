{
    "type": "origins:action_over_time",
    "interval": 200,
    "entity_action": {
        "type": "origins:execute_command",
        "command": "scale set extraorigins:modify_size 0.85"
    },
    "condition": {
        "type": "origins:and",
        "conditions": [
            {
                "type": "origins:resource",
                "resource": "mysticorigins:lifecycle",
                "comparison": "<=",
                "compare_to": 119
            },
            {
                "type": "origins:resource",
                "resource": "mysticorigins:lifecycle",
                "comparison": ">=",
                "compare_to": 80
            }
        ]
    },
    "hidden": true
}